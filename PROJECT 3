SELECT 
	 PRODUCTLINE, 
	 YEAR_ID, 
	 DEALSIZE, 
	 SUM(sales) AS REVENUE
FROM SALES_DATASET_RFM_PRJ_CLEAN
GROUP BY PRODUCTLINE, 
	 YEAR_ID, 
	 DEALSIZE

SELECT 
	 MONTH_ID, 
	 SUM(sales) AS REVENUE, 
	 ORDERNUMBER
FROM SALES_DATASET_RFM_PRJ_CLEAN
GROUP BY MONTH_ID,
	ORDERNUMBER
ORDER BY REVENUE DESC
LIMIT 1
-> Tháng 10 là tháng bán tốt nhất 

SELECT * FROM SALES_DATASET_RFM_PRJ_CLEAN

SELECT 
	PRODUCTLINE,
	 MONTH_ID, 
	 SUM(sales) AS REVENUE, 
	 ORDERNUMBER
FROM SALES_DATASET_RFM_PRJ_CLEAN
WHERE MONTH_ID = 11
GROUP BY MONTH_ID,
	ORDERNUMBER,
	PRODUCTLINE
ORDER BY ORDERNUMBER DESC
LIMIT  1
-> Vintage Cars bán được nhiều nhất trong tháng 11

SELECT 
	YEAR_ID, 
	PRODUCTLINE,
	SUM(sales) AS REVENUE,
	RANK() OVER(PARTITION BY YEAR_ID ORDER BY SUM(sales) DESC) AS RANK
FROM SALES_DATASET_RFM_PRJ_CLEAN
WHERE COUNTRY = 'UK'
GROUP BY YEAR_ID, 
	PRODUCTLINE
